<launch>
	<rosparam file="$(find quadruped_locomotion)/config/anymal_config.yaml" command="load"/>

	<include file="$(find gazebo_ros)/launch/empty_world.launch"/>
	<!-- Load anymal urdf into ROS parameter server -->
	<include file="$(find anymal_c_simple_description)/launch/load.launch"/>

	<!-- Spawn anymal into Gazebo -->
	<node name="spawn_urdf" pkg="gazebo_ros" type="spawn_model" args="-param anymal_description -urdf -model anymal -z 5 -R 3.1416"/>

	<!--<node name="anymal_controller" pkg="quadruped_locomotion" type="controller_node"/>-->
</launch>
